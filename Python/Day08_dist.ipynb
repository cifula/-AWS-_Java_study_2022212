    "import numpy as np\n",
    "import math\n",
    "\n",
    "x1 = 3\n",
    "y1 = 10\n",
    "\n",
    "x2 = 5\n",
    "y2 = 25\n",
    "\n",
    "def dist(p1, p2):\n",
    "    p1 = np.array(p1)\n",
    "    p2 = np.array(p2)\n",
    "    \n",
    "    r = p1 - p2\n",
    "    r = math.sqrt(sum(r * r))\n",
    "    return r\n",
    "\n",
    "dist([x1, y1], [x2, y2])"
   ]
  }
